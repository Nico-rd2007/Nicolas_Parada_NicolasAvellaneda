Algoritmo hola
	Definir nombre Como Caracter;
	Definir deporte Como Caracter;
	Definir edad Como entero;
	Escribir "ingresa el nombre de tu deportista favorito";
	Leer nombre;
	Escribir "deporte en el que se destaca";
	Leer deporte;
	Escribir "ingresar su edad";
	Leer edad;
	Escribir "El deportista se llama ", nombre, ". El deporte en que se destaca es ", deporte, " su edad es ", edad;
FinAlgoritmo